  "Serilog": {
    "Using": [ "Serilog.Sinks.Graylog"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "127.0.0.1",
