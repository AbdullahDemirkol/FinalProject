@page "/logout"

@using System.Web


<h3>LogoutPage</h3>

@code {
    [Inject]
    IIdentityService identityService { get; set; }

    [Inject]
    NavigationManager navigationManager { get; set; }


    protected override void OnInitialized()
    {
        identityService.Logout();
        navigationManager.NavigateTo(navigationManager.BaseUri);
    }
}
